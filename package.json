{
    "name": "puloba-settings",
    "version": "1.0.0",
    "description": "puloba chrome extension settings page",
    "author": "Jaewook Ahn",
    "license": "MIT",
    "scripts": {
      "package": "echo \"Not supported yet!\"",
      "build-settings": "cd settings && yarn build",
