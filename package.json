      "lint-extension": "cd extension && eslint",
      "lint": "yarn lint-setings && yarn lint-extension"
    },
    "devDependencies": {
      "@types/node": "^13.9.8",
      "@types/react": "^16.9.31",
      "@types/react-dom": "^16.9.6",
      "eslint": "^6.8.0",
      "typescript": "^3.8.3"
    },
    "dependencies": {
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-scripts": "^3.4.1"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }
